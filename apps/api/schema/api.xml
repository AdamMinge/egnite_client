<?xml version="1.0" encoding="UTF-8"?>
<Api name="ExampleApi">
	<Path>api</Path>
	<Include>model.h</Include>
	<Header key="global_header" value="global_header_value"/>
	<Parameter key="global_parameter" value="global_parameter_value"/>
	<Method name="getExamples" verb="GET" returns="QList&lt;ExampleModel&gt;"/>
	<Method name="getExample" verb="GET" returns="ExampleModel">
		<Argument key="id" type="int"/>
		<Header key="header" value="header_value"/>
